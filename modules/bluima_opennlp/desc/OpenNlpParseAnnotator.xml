<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
	<frameworkImplementation>org.apache.uima.java</frameworkImplementation>
	<primitive>true</primitive>
	<annotatorImplementationName>de.julielab.jules.ae.opennlp.ParseAnnotator
	</annotatorImplementationName>
	<analysisEngineMetaData>
		<name>jules-opennlp-parser-ae</name>
		<description></description>
		<version>2.1</version>
		<vendor>julielab</vendor>
		<configurationParameters>
			<configurationParameter>
				<name>modelDir</name>
				<type>String</type>
				<multiValued>false</multiValued>
				<mandatory>false</mandatory>
			</configurationParameter>
			<configurationParameter>
				<name>useTagdict</name>
				<type>Boolean</type>
				<multiValued>false</multiValued>
				<mandatory>false</mandatory>
			</configurationParameter>
			<configurationParameter>
				<name>caseSensitive</name>
				<type>Boolean</type>
				<multiValued>false</multiValued>
				<mandatory>false</mandatory>
			</configurationParameter>
			<configurationParameter>
				<name>beamSize</name>
				<type>Integer</type>
				<multiValued>false</multiValued>
				<mandatory>false</mandatory>
			</configurationParameter>
			<configurationParameter>
				<name>advancePercentage</name>
				<type>String</type>
				<multiValued>false</multiValued>
				<mandatory>false</mandatory>
			</configurationParameter>
			<configurationParameter>
				<name>fun</name>
				<type>Boolean</type>
				<multiValued>false</multiValued>
				<mandatory>false</mandatory>
			</configurationParameter>
			<configurationParameter>
				<name>tagset</name>
				<type>String</type>
				<multiValued>false</multiValued>
				<mandatory>false</mandatory>
			</configurationParameter>
			<configurationParameter>
				<name>mappings</name>
				<type>String</type>
				<multiValued>true</multiValued>
				<mandatory>false</mandatory>
			</configurationParameter>
		</configurationParameters>
		<configurationParameterSettings>
			<nameValuePair>
				<name>useTagdict</name>
				<value>
					<boolean>true</boolean>
				</value>
			</nameValuePair>
			<nameValuePair>
				<name>caseSensitive</name>
				<value>
					<boolean>false</boolean>
				</value>
			</nameValuePair>
			<nameValuePair>
				<name>fun</name>
				<value>
					<boolean>true</boolean>
				</value>
			</nameValuePair>
			<nameValuePair>
				<name>modelDir</name>
				<value>
					<string>resources/parserGENIA/</string>
				</value>
			</nameValuePair>
			<nameValuePair>
				<name>tagset</name>
				<value>
					<string>de.julielab.jules.types.GENIAConstituent</string>
				</value>
			</nameValuePair>
			<nameValuePair>
				<name>mappings</name>
				<value>
					<array>
						<string>S;S</string>
						<string>SBAR;SBAR</string>
						<string>SBARQ;SBARQ</string>
						<string>SINV;SINV</string>
						<string>SQ;SQ</string>
						<string>ADVP;ADVP</string>
						<string>CONJP;CONJP</string>
						<string>FRAG;FRAG</string>
						<string>LST;LST</string>
						<string>NP;NP</string>
						<string>PP;PP</string>
						<string>PRN;PRN</string>
						<string>PRT;PRT</string>
						<string>QP;QP</string>
						<string>RRC;RRC</string>
						<string>UCP;UCP</string>
						<string>VP;VP</string>
						<string>WHADJP;WHADJP</string>
						<string>WHAVP;WHAVP</string>
						<string>WHNP;WHNP</string>
						<string>WHPP;WHPP</string>
						<string>NAC;NAC</string>
						<string>COMP;COMP</string>
						<string>NML;NML</string>
						<string>INTJ;INTJ</string>
						<string>NX;NX</string>
						<string>X;X</string>
					</array>
				</value>
			</nameValuePair>
		</configurationParameterSettings>
		<!-- ren
		<typeSystemDescription>
			<imports>
				<import location="julie-morpho-syntax-types.xml" />
			</imports>
		</typeSystemDescription>
		 -->
		<typePriorities />
		<fsIndexCollection />
		<capabilities>
			<capability>
				<inputs>
					<type allAnnotatorFeatures="true">de.julielab.jules.types.Token
					</type>
					<type allAnnotatorFeatures="true">de.julielab.jules.types.Sentence
					</type>
				</inputs>
				<outputs>
					<type allAnnotatorFeatures="true">de.julielab.jules.types.Constituent
					</type>
				</outputs>
				<languagesSupported />
			</capability>
		</capabilities>
		<operationalProperties>
			<modifiesCas>true</modifiesCas>
			<multipleDeploymentAllowed>true</multipleDeploymentAllowed>
			<outputsNewCASes>false</outputsNewCASes>
		</operationalProperties>
	</analysisEngineMetaData>
	<resourceManagerConfiguration />
</analysisEngineDescription>
