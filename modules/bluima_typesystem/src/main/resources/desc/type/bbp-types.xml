<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>bbp-basic-types</name>
  <description>The type system contains the basic annotation types.</description>
  <version>2.6</version>
  <vendor>BlueBrain project, EPFL</vendor>
  <imports>
    <import location="julie-basic-types.xml"/>
    <import location="julie-morpho-syntax-types.xml"/>
    <import location="julie-semantics-biology-types.xml"/>
    <import location="DictTerm.xml"/>
  </imports>
  <types>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.SurfaceForm</name>
      <description>Surface form from BioLexicon's FORMS table</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>pos</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>semtype</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.POSVerb</name>
      <description/>
      <supertypeName>de.julielab.jules.types.POSTag</supertypeName>
      <features>
        <featureDescription>
          <name>biolexicon_id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.POSWh</name>
      <description/>
      <supertypeName>de.julielab.jules.types.POSTag</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.POSSkip</name>
      <description/>
      <supertypeName>de.julielab.jules.types.POSTag</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.POSAdverb</name>
      <description/>
      <supertypeName>de.julielab.jules.types.POSTag</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.DataTable</name>
      <description>A table that contains data, extracted from a pdf article</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>tableId</name>
          <description>the id of this table in the document</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>rowCount</name>
          <description>the nr of rows</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>columnCount</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>caption</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>headings</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>body</name>
          <description>the body of the table that contains data</description>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>referenceText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>pageNumber</name>
          <description>the number of the page where the table is located in</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.DocumentElement</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>ElementId</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>isBold</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>height</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>width</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>x</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>y</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>pageId</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>label</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>medianFontsize</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.DocumentLine</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DocumentElement</supertypeName>
      <features>
        <featureDescription>
          <name>block</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lineText</name>
          <description>workaround to get the line text</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>beginnings</name>
          <description/>
          <rangeTypeName>uima.cas.FloatArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endings</name>
          <description/>
          <rangeTypeName>uima.cas.FloatArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.DocumentBlock</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DocumentElement</supertypeName>
      <features>
        <featureDescription>
          <name>hasBold</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>hasManyFontsizes</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Punctuation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.GeneralEnglish</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.DocumentTextHolder</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>text</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Verb</name>
      <description>A general verb, that was not found in the biolexicon/Mancu</description>
      <supertypeName>de.julielab.jules.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>isModal</name>
          <description>whether this verb has the POS MD (modal), e.g. should, ...</description>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Noise</name>
      <description>A generic annotation for text that is not relevant</description>
      <supertypeName>de.julielab.jules.types.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Stopword</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.Noise</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.FrequentToken</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.Noise</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Hapax</name>
      <description>a word which occurs only once within a context</description>
      <supertypeName>ch.epfl.bbp.uima.types.Noise</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.BodyCitation</name>
      <description>A literature citation found in the body of the text, not the reference section</description>
      <supertypeName>de.julielab.jules.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>textValue</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.FeatureTokens</name>
      <description>a list of strings that represent the cleaned-up (pre-processed) tokens for use with LDA libs or others</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>tokens</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>beginnings</name>
          <description/>
          <rangeTypeName>uima.cas.IntegerArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endings</name>
          <description/>
          <rangeTypeName>uima.cas.IntegerArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Figure</name>
      <description>A figure in the text</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Keep</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>enclosedAnnot</name>
          <description/>
          <rangeTypeName>uima.tcas.Annotation</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>normalizedText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Gold</name>
      <description>The gold annotation, coming from a corpus</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>typeS</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.BadContent</name>
      <description>Document-level annotation signaling that this document has content that is bad and should not be (futher) analyzed.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.TooManyOOV</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.BadContent</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.TooFewTokensPerPage</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.BadContent</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.EtAlInlineReference</name>
      <description>Satre et al. (2007)
Zucker et al. (1993)</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.DocumentPage</name>
      <description>Represents a page in a document, e.g. a page in a PDF article</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>pageId</name>
          <description>The id of the page, 0-indexed</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Protein</name>
      <description/>
      <supertypeName>de.julielab.jules.types.Protein</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.CellType</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Measure</name>
      <description>a measure, e.g. 40g</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>textValue</name>
          <description>the complete surface form of this measure</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>value</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>normalizedValue</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>rangeStartValue</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>rangeEndValue</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>meanValue</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>standardErrorValue</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>accuracy</name>
          <description>allows to store additional information about this
            measure, e.g. modifiers like '~', '+/-', 'approximately'</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>unit</name>
          <description>iso</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>normalizedUnit</name>
          <description>iso</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>characterizes the confidence of the extractor
            that this is indeed a measure. default should be 1f</description>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.Concentration</name>
      <description>a concentration, e.g. 40mM</description>
      <supertypeName>ch.epfl.bbp.uima.types.Measure</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.CellTypeProteinConcentration</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>protein</name>
          <description/>
          <rangeTypeName>ch.epfl.bbp.uima.types.Protein</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>concentration</name>
          <description/>
          <rangeTypeName>ch.epfl.bbp.uima.types.Concentration</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>celltype</name>
          <description/>
          <rangeTypeName>ch.epfl.bbp.uima.types.CellType</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.genia.Event</name>
      <description>See
        http://2011.bionlp-st.org/home/genia-event-extraction-genia</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>event_type</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>themes_protein</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>ch.epfl.bbp.uima.types.Protein</elementType>
        </featureDescription>
        <featureDescription>
          <name>causes_protein</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>ch.epfl.bbp.uima.types.Protein</elementType>
        </featureDescription>
        <featureDescription>
          <name>site</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>themes_event</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>ch.epfl.bbp.uima.genia.Event</elementType>
        </featureDescription>
        <featureDescription>
          <name>causes_event</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>ch.epfl.bbp.uima.genia.Event</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.BrainRegion</name>
      <description/>
      <supertypeName>de.julielab.jules.types.BioEntityMention</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.BioVerb</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>ref_id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>canonical</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>enclosingSpan</name>
          <description>span that this NoTerm is contained within (i.e.
            its sentence)</description>
          <rangeTypeName>uima.tcas.Annotation</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>matchedText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>matchedTokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.Sex</name>
      <description/>
      <supertypeName>de.julielab.jules.types.BioEntityMention</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.AgeDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.CellDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.OrganismDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.RegionDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.SexDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.SubcellDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.UniprotDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.TheontologyDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.VerbDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.ProteinDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.BiolexiconDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.DictTerm</name>
      <description/>
      <supertypeName>org.apache.uima.conceptMapper.DictTerm</supertypeName>
      <features>
        <featureDescription>
          <name>entityId</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>annotType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.TopicDistribution</name>
      <description>The distr of topic from a LDA analysis on that document</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>probability</name>
          <description/>
          <rangeTypeName>uima.cas.DoubleArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.MultipleProteins</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>present</name>
          <description>1 if present</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ch.epfl.bbp.uima.types.IonchannelDictTerm</name>
      <description/>
      <supertypeName>ch.epfl.bbp.uima.types.DictTerm</supertypeName>
